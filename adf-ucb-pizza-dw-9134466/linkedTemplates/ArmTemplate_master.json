{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Nombre de instancia de Data Factory",
			"defaultValue": "adf-ucb-pizza-dw-9134466"
		},
		"LS_SQL_DW_PizzaSales_connectionString": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"connectionString\"de \"LS_SQL_DW_PizzaSales\""
		},
		"LS_SQL_DW_PizzaSales_Clean_password": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"password\"de \"LS_SQL_DW_PizzaSales_Clean\""
		},
		"LS_Storage_PizzaSales_connectionString": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"connectionString\"de \"LS_Storage_PizzaSales\""
		},
		"LS_Storage_PizzaSales_Clean_connectionString": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"connectionString\"de \"LS_Storage_PizzaSales_Clean\""
		},
		"LS_SQL_DW_PizzaSales_Clean_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sql-ucb-pizza-dw-9134466.database.windows.net"
		},
		"LS_SQL_DW_PizzaSales_Clean_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "dw_pizza_sales"
