					"LS_SQL_DW_PizzaSales_Clean_password": {
						"value": "[parameters('LS_SQL_DW_PizzaSales_Clean_password')]"
					},
					"LS_Storage_PizzaSales_connectionString": {
						"value": "[parameters('LS_Storage_PizzaSales_connectionString')]"
					},
					"LS_Storage_PizzaSales_Clean_connectionString": {
						"value": "[parameters('LS_Storage_PizzaSales_Clean_connectionString')]"
					},
					"LS_SQL_DW_PizzaSales_Clean_properties_typeProperties_server": {
